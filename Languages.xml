		<author>Kevin Rapley</author>
		<identifier>com.digikev.XML.XSLT</identifier>
		<version status="preview">0.0a1</version>
		<download format="git">git://github.com/DigiKev/XSLT-Espresso-Sugars.git</download>
		<url>http://github.com/DigiKev/XSLT-Espresso-Sugars</url>
		<dependencies>
			<dependency id="espresso.default.XML">XML</dependency>
			<dependency id="espresso.default.HTML">HTML</dependency>
		</dependencies>	
