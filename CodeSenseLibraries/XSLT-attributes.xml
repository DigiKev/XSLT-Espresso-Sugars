		<completion string="lang" />
		<completion string="data-type" />
		<completion string="order" />
		<completion string="case-order" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:strip-space.attributes">
		<completion string="elements" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:stylesheet.attributes">
		<completion string="id" />
		<completion string="extension-element-prefixes" />
		<completion string="exclude-result-prefixes" />
		<completion string="version" />
