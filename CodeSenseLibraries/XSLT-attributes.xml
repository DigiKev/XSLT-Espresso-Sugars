	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:value-of.attributes">
		<completion string="select" />
		<completion string="disable-output-escaping" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:variable.attributes">
		<completion string="name" />
		<completion string="select" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:when.attributes">
