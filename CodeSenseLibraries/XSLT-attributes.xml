		<completion string="result-prefix" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:number.attributes">
		<completion string="level" />
		<completion string="count" />
		<completion string="from" />
		<completion string="value" />
		<completion string="format" />
		<completion string="lang" />
		<completion string="letter-value" />
		<completion string="grouping-separator" />
		<completion string="grouping-size" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:output.attributes">
		<completion string="method" />
		<completion string="version" />
		<completion string="encoding" />
		<completion string="omit-xml-declaration" />
		<completion string="standalone" />
		<completion string="doctype-public" />
		<completion string="doctype-system" />
		<completion string="cdata-section-elements" />
		<completion string="indent" />
		<completion string="media-type" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:param.attributes">
		<completion string="name" />
		<completion string="select" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:preserve-space.attributes">
		<completion string="elements" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:processing-instruction.attributes">
		<completion string="name" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:sort.attributes">
