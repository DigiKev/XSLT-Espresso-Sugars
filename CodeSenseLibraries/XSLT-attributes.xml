<?xml version="1.0" encoding="UTF-8"?>
<!--

CodeSense Libraries simply define lists of completions, categorized by keys. They don't define any criteria for when they should be used; this is the domain of a CodeSense Provider.

-->
<codesense version="1.0">

	<behavior>
		<append-dynamic>
			<matched-suffix>(\s*=)|[^=]</matched-suffix>
			<transform-into>(?1::="$1"$0)</transform-into>
		</append-dynamic>
	</behavior>
		
	<set name="com.macrabbit.xslt.tag.xsl:apply-templates.attributes">
		<completion string="select" />
		<completion string="mode" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:attribute.attributes">
		<completion string="name" />
		<completion string="namespace" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:attribute-set.attributes">
		<completion string="name" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:call-template.attributes">
		<completion string="name" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:copy.attributes">
		<completion string="use-attribute-sets" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:copy-of.attributes">
		<completion string="select" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:decimal-format.attributes">
		<completion string="name" />
		<completion string="decimal-separator" />
		<completion string="grouping-separator" />
		<completion string="infinity" />
		<completion string="minus-sign" />
		<completion string="NaN" />
		<completion string="percent" />
		<completion string="per-mile" />
		<completion string="zero-digit" />
		<completion string="digit" />
		<completion string="pattern-separator" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:element.attributes">
		<completion string="name" />
		<completion string="name-space" />
		<completion string="use-attribute-sets" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:for-each.attributes">
		<completion string="select" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:if.attributes">
		<completion string="test" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:import.attributes">
		<completion string="href" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:include.attributes">
		<completion string="href" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:key.attributes">
		<completion string="name" />
		<completion string="match" />
		<completion string="use" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:message.attributes">
		<completion string="terminate" />
	</set>
	
	<set name="com.macrabbit.xslt.tag.xsl:namespace-alias.attributes">
